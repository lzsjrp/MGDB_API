{
  "name": "express",
  "type": "module",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": true
  },
  "scripts": {
    "dev": "bun run prisma:generate && bun run prisma:migrate && bun run --watch src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "build": "tsup",
    "start": "bun run dist/index.cjs"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@supabase/supabase-js": "^2.75.0",
    "@types/uuid": "^11.0.0",
    "bcrypt": "^6.0.0",
    "express": "5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/express": "5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "20.11.17",
    "prisma": "^6.17.1",
    "tsup": "^8.5.0",
    "typescript": "5.8.3"
  }
}